  <eClassifiers xsi:type="ecore:EClass" name="ErrorPortIn" eSuperTypes="#//ErrorPortDec">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ErrorPortInSuperType" eType="#//PortType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ErrorPortOut" eSuperTypes="#//ErrorPortDec">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ErrorPortOutSuperType"
        eType="#//PortType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StateDec" eSuperTypes="#//ErrorModelElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="TransitionDec" eSuperTypes="#//ErrorModelElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="Features" upperBound="-1"
        eType="#//TransitionFeatureDec" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TransitionFeatureDec"/>
  <eClassifiers xsi:type="ecore:EClass" name="TransitionState" eSuperTypes="#//TransitionFeatureDec">
    <eStructuralFeatures xsi:type="ecore:EReference" name="sourceState" eType="#//StateDec"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="targetState" eType="#//StateDec"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TriggerDec" eSuperTypes="#//TransitionFeatureDec">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="portInstance" eType="#//PortDec"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="errorMode" eType="#//ErrorModes"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ActionDec" eSuperTypes="#//TransitionFeatureDec">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="portInstance" eType="#//PortDec"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="errorMode" eType="#//ErrorModes"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OccurrenceDec" eSuperTypes="#//TransitionFeatureDec"/>
</ecore:EPackage>
